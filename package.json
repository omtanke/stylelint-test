{
  "name": "cra5",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "audit": "yarn audit",
    "outdated": "yarn outdated",
    "stylelint": "stylelint **/*.scss",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.0-alpha.4",
    "@storybook/addon-essentials": "^6.5.0-alpha.4",
    "@storybook/addon-links": "^6.5.0-alpha.4",
    "@storybook/builder-webpack5": "^6.5.0-alpha.4",
    "@storybook/manager-webpack5": "^6.5.0-alpha.4",
    "@storybook/node-logger": "^6.5.0-alpha.4",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.5.0-alpha.3",
    "sass": "^1.45.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "webpack": "^5.65.0"
  },
  "resolutions": {
    "browserslist": ">=4.16.5",
    "glob-parent": ">=5.1.2",
    "immer": ">=9.0.6",
    "nth-check": ">=2.0.1",
    "trim": "^1.0.1",
    "trim-newlines": "^4.0.2"
  }
